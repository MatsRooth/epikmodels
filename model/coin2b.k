world = H1+T1+H2+T2
assert (~(H1&T1))&(H1+T1)&(~(H2&T2))&(H2+T2)
action announce_H1 = (test H1;id;test H1) & ((test H2;id;test H2)+(test T2;id;test T2))
action announce_T1 = (test T1;id;test T1) & ((test H2;id;test H2)+(test T2;id;test T2))
action announce_H2 = (test H2;id;test H2) & ((test H1;id;test H1)+(test T1;id;test T1))
action announce_T2 = (test T2;id;test T2) & ((test H1;id;test H1)+(test T1;id;test T1))
action peek_amy_H1 = (test H1;id;test H1) & ((test H2;id;test H2)+(test T2;id;test T2))
action peek_amy_T1 = (test T1;id;test T1) & ((test H2;id;test H2)+(test T2;id;test T2))
action peek_amy_H2 = (test H2;id;test H2) & ((test H1;id;test H1)+(test T1;id;test T1))
action peek_amy_T2 = (test T2;id;test T2) & ((test H1;id;test H1)+(test T1;id;test T1))
action peek_bob_H1 = (test H1;id;test H1) & ((test H2;id;test H2)+(test T2;id;test T2))
action peek_bob_T1 = (test T1;id;test T1) & ((test H2;id;test H2)+(test T2;id;test T2))
action peek_bob_H2 = (test H2;id;test H2) & ((test H1;id;test H1)+(test T1;id;test T1))
action peek_bob_T2 = (test T2;id;test T2) & ((test H1;id;test H1)+(test T1;id;test T1))


agent amy =
 (announce_H1 -> announce_H1)
 (announce_T1 -> announce_T1)
 (announce_H2 -> announce_H2)
 (announce_T2 -> announce_T2)
 (peek_amy_H1 -> peek_amy_H1)
 (peek_amy_T1 -> peek_amy_T1)
 (peek_amy_H2 -> peek_amy_H2)
 (peek_amy_T2 -> peek_amy_T2)
 (peek_bob_H1 -> peek_bob_H1 + peek_bob_T1)
 (peek_bob_T1 -> peek_bob_H1 + peek_bob_T1)
 (peek_bob_H2 -> peek_bob_H2 + peek_bob_T2)
 (peek_bob_T2 -> peek_bob_H2 + peek_bob_T2)

agent bob =
 (announce_H1 -> announce_H1)
 (announce_T1 -> announce_T1)
 (announce_H2 -> announce_H2)
 (announce_T2 -> announce_T2)
 (peek_amy_H1 -> peek_amy_H1 + peek_amy_T1)
 (peek_amy_T1 -> peek_amy_H1 + peek_amy_T1)
 (peek_amy_H2 -> peek_amy_H2 + peek_amy_T2)
 (peek_amy_T2 -> peek_amy_H2 + peek_amy_T2)
 (peek_bob_H1 -> peek_bob_H1)
 (peek_bob_T1 -> peek_bob_T1)
 (peek_bob_H2 -> peek_bob_H2)
 (peek_bob_T2 -> peek_bob_T2)

queryall [bob](_; _; peek_amy_T1; peek_amy_T2)
